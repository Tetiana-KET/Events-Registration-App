{
  "name": "events-registration-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src/ --fix",
    "preview": "vite build && vite preview",
    "prettier": "prettier --check --ignore-unknown src/",
    "format:fix": "prettier --write --ignore-unknown src/",
    "types-check": "tsc --noEmit",
    "format:all": "npm run lint:fix && npm run prettier:fix",
    "validate:all": "npm run lint && npm run prettier && npm run types-check",
    "prepare": "husky",
    "deploy": "npm run build && npx gh-pages -d dist -e Events-Registration-App"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/material": "^6.1.1",
    "eslint": "^9.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-router-dom": "^6.26.0",
    "vite-tsconfig-paths": "^5.0.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0"
  }
}
